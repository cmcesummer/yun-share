{
    "liveServer.settings.root": "/dist/",
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "build/": true,
        "temp/": true,
        "library/": true,
        "**/*.anim": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.meta": true,
        "library/": true,
        "local/": true,
        "temp/": true
    },
    "[javascript]": {
        "editor.tabSize": 4
    },
    "editor.detectIndentation": false,
    "editor.smoothScrolling": true,
    "editor.formatOnSave": true,
    "fileheader.Author": "cmce",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "emmet.syntaxProfiles": {
        "javascript": "jsx"
    },
    "liveServer.settings.port": 7010,
    "git.ignoreLimitWarning": true,
    "liveServer.settings.multiRootWorkspaceName": "rssr",
    "fileheader.LastModifiedBy": "cmce"
}
